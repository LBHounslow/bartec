{
  "name": "lb-hounslow/bartec",
  "description": "A client for the Bartec Collective API v15 SOAP Web Service",
  "type": "library",
  "license": "proprietary",
  "require": {
    "php": "^7.2",
    "ext-json": "*",
    "ext-soap": "*"
  },
  "autoload": {
    "psr-4": {
      "Bartec\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/",
      "Tests\\Unit\\": "tests/unit/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5.2"
  }
}
